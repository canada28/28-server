<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.game.dao.PlaygroundMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.game.model.Playground">
        <id column="id" property="id" />
        <result column="gid" property="gid" />
        <result column="title" property="title" />
        <result column="odds_explain" property="oddsExplain" />
        <result column="game_rule" property="gameRule" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, gid, title, odds_explain, game_rule, status, create_time
    </sql>

</mapper>
